#: ../../../advanced_features/sgl_model_gateway.md:1438
msgid "**Essential Dashboards:**"
msgstr ""

#: ../../../advanced_features/sgl_model_gateway.md:1440
msgid "**1. Request Rate and Latency:**"
msgstr ""

#: ../../../advanced_features/sgl_model_gateway.md:1441
msgid ""
"# Request rate by endpoint\n"
"sum(rate(smg_http_requests_total[5m])) by (path, method)\n"
"\n"
"# P50 latency\n"
"histogram_quantile(0.50, "
"sum(rate(smg_http_request_duration_seconds_bucket[5m])) by (le))\n"
"\n"
"# P99 latency\n"
"histogram_quantile(0.99, "
"sum(rate(smg_http_request_duration_seconds_bucket[5m])) by (le))\n"
"\n"
"# Error rate\n"
"sum(rate(smg_http_responses_total{status=~\"5..\"}[5m])) / "
"sum(rate(smg_http_responses_total[5m]))\n"
msgstr ""

#: ../../../advanced_features/sgl_model_gateway.md:1455
msgid "**2. Worker Health:**"
msgstr ""

#: ../../../advanced_features/sgl_model_gateway.md:1456
msgid ""
"# Healthy workers\n"
"sum(smg_worker_pool_size)\n"
"\n"
"# Active connections per worker\n"
"smg_worker_connections_active\n"
"\n"
"# Worker health check failures\n"
"sum(rate(smg_worker_health_checks_total{result=\"failure\"}[5m])) by "
"(worker_id)\n"
msgstr ""

#: ../../../advanced_features/sgl_model_gateway.md:1467
msgid "**3. Circuit Breaker Status:**"
msgstr ""

#: ../../../advanced_features/sgl_model_gateway.md:1468
msgid ""
"# Circuit breaker states (0=closed, 1=open, 2=half-open)\n"
"smg_worker_cb_state\n"
"\n"
"# Circuit breaker transitions\n"
"sum(rate(smg_worker_cb_transitions_total[5m])) by (worker_id, from_state, "
"to_state)\n"
"\n"
"# Workers with open circuits\n"
"count(smg_worker_cb_state == 1)\n"
msgstr ""

#: ../../../advanced_features/sgl_model_gateway.md:1479
msgid "**4. Inference Performance (gRPC mode):**"
msgstr ""

#: ../../../advanced_features/sgl_model_gateway.md:1480
msgid ""
"# Time to first token (P50)\n"
"histogram_quantile(0.50, sum(rate(smg_router_ttft_seconds_bucket[5m])) by "
"(le, model))\n"
"\n"
"# Time per output token (P99)\n"
"histogram_quantile(0.99, sum(rate(smg_router_tpot_seconds_bucket[5m])) by "
"(le, model))\n"
"\n"
"# Token throughput\n"
"sum(rate(smg_router_tokens_total[5m])) by (model, direction)\n"
"\n"
"# Generation duration P95\n"
"histogram_quantile(0.95, "
"sum(rate(smg_router_generation_duration_seconds_bucket[5m])) by (le))\n"
msgstr ""

#: ../../../advanced_features/sgl_model_gateway.md:1494
msgid "**5. Rate Limiting and Queuing:**"
msgstr ""

#: ../../../advanced_features/sgl_model_gateway.md:1495
msgid ""
"# Rate limit rejections\n"
"sum(rate(smg_http_rate_limit_total{decision=\"rejected\"}[5m]))\n"
"\n"
"# Queue depth (if using concurrency limiting)\n"
"smg_worker_requests_active\n"
"\n"
"# Retry attempts\n"
"sum(rate(smg_worker_retries_total[5m])) by (worker_id)\n"
"\n"
"# Exhausted retries (failures after all retries)\n"
"sum(rate(smg_worker_retries_exhausted_total[5m]))\n"
msgstr ""

#: ../../../advanced_features/sgl_model_gateway.md:1509
msgid "**6. MCP Tool Execution:**"
msgstr ""

#: ../../../advanced_features/sgl_model_gateway.md:1510
msgid ""
"# Tool call rate\n"
"sum(rate(smg_mcp_tool_calls_total[5m])) by (server, tool)\n"
"\n"
"# Tool latency P95\n"
"histogram_quantile(0.95, sum(rate(smg_mcp_tool_duration_seconds_bucket[5m])) "
"by (le, tool))\n"
"\n"
"# Active MCP server connections\n"
"smg_mcp_servers_active\n"
msgstr ""

#: ../../../advanced_features/sgl_model_gateway.md:1521
msgid "**Alerting Rules Example:**"
msgstr ""

#: ../../../advanced_features/sgl_model_gateway.md:1523
msgid ""
"groups:\n"
"- name: sglang-gateway\n"
"  rules:\n"
"  - alert: HighErrorRate\n"
"    expr: |\n"
"      sum(rate(smg_http_responses_total{status=~\"5..\"}[5m]))\n"
"      / sum(rate(smg_http_responses_total[5m])) > 0.05\n"
"    for: 5m\n"
"    labels:\n"
"      severity: critical\n"
"    annotations:\n"
"      summary: \"High error rate on SGLang Gateway\"\n"
"\n"
"  - alert: CircuitBreakerOpen\n"
"    expr: count(smg_worker_cb_state == 1) > 0\n"
"    for: 2m\n"
"    labels:\n"
"      severity: warning\n"
"    annotations:\n"
"      summary: \"Worker circuit breaker is open\"\n"
"\n"
"  - alert: HighLatency\n"
"    expr: |\n"
"      histogram_quantile(0.99, "
"sum(rate(smg_http_request_duration_seconds_bucket[5m])) by (le)) > 30\n"
"    for: 5m\n"
"    labels:\n"
"      severity: warning\n"
"    annotations:\n"
"      summary: \"P99 latency exceeds 30 seconds\"\n"
"\n"
"  - alert: NoHealthyWorkers\n"
"    expr: sum(smg_worker_pool_size) == 0\n"
"    for: 1m\n"
"    labels:\n"
"      severity: critical\n"
"    annotations:\n"
"      summary: \"No healthy workers available\"\n"
msgstr ""

