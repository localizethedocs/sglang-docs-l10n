msgid "Quick Start"
msgstr ""

#: ../../../performance_dashboard/README.md:15
msgid "Option 1: Run with Local Server (Recommended)"
msgstr ""

#: ../../../performance_dashboard/README.md:17
msgid "For live data from GitHub Actions artifacts:"
msgstr ""

#: ../../../performance_dashboard/README.md:19
msgid ""
"# Install requirements\n"
"pip install requests\n"
"\n"
"# Run the server\n"
"python server.py --fetch-on-start\n"
"\n"
"# Visit http://localhost:8000\n"
msgstr ""

#: ../../../performance_dashboard/README.md:29
msgid "The server provides:"
msgstr ""

#: ../../../performance_dashboard/README.md:30
msgid "Automatic fetching of metrics from GitHub"
msgstr ""

#: ../../../performance_dashboard/README.md:31
msgid "Caching to reduce API calls"
msgstr ""

#: ../../../performance_dashboard/README.md:32
msgid "`/api/metrics` endpoint for the frontend"
msgstr ""

#: ../../../performance_dashboard/README.md:34
msgid "Option 2: Fetch Data Manually"
msgstr ""

#: ../../../performance_dashboard/README.md:36
msgid "Use the fetch script to download metrics data:"
msgstr ""

#: ../../../performance_dashboard/README.md:38
msgid ""
"# Fetch last 30 days of metrics\n"
"python fetch_metrics.py --output metrics_data.json\n"
"\n"
"# Fetch a specific run\n"
"python fetch_metrics.py --run-id 21338741812 --output single_run.json\n"
"\n"
"# Fetch only scheduled (nightly) runs\n"
"python fetch_metrics.py --scheduled-only --days 7\n"
msgstr ""

