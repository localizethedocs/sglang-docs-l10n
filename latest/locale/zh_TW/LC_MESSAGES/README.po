msgstr ""

#: ../../../README.md:8
msgid "Install Dependency"
msgstr "安裝相依項"

#: ../../../README.md:10
msgid ""
"apt-get update && apt-get install -y pandoc parallel retry\n"
"pip install -r requirements.txt\n"
msgstr ""
"apt-get update && apt-get install -y pandoc parallel retry\n"
